# Append proper formatting to .gitattributes
echo "/mvnw text eol=lf" > .gitattributes
echo "*.cmd text eol=crlf" >> .gitattributes

# Add the changes
git add .gitattributes

# Commit the changes with a meaningful message
git commit -m "Fixed .gitattributes formatting and line endings"

# Push to the branch
git push origin master
